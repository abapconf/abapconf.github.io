                <Text text="{i18n>agendaTablePause}" class="whiteText"/>
            </ColumnListItem>
            <ColumnListItem class="columnPause">
                <Text text="16:30" class="whiteText"/>
                <Text text="{i18n>agendaTableBack}" class="whiteText"/>
            </ColumnListItem>
            <ColumnListItem>
