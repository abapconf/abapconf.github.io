            </ColumnListItem>
            <ColumnListItem>
                <Text text="15:10" />  
                <!-- V106 --> 
                <VBox blocked="{agendaView>/showGerman}" class="sapUiSmallMarginBottom sapUiSmallMarginTop">
                    <Link 
